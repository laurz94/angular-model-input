{
  "bracketSpacing": true,
  "bracketSameLine": false,
  "endOfLine": "crlf",
  "htmlWhitespaceSensitivity": "css",
  "printWidth": 140,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false,
  "overrides": [
    {
      "files": "*html",
      "options": { "parser": "angular" }
    }
  ]
}
